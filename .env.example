# Google Cloud Project ID
GCP_PROJECT=your-project-id

# Notion API Secret
NOTION_SECRET=your-notion-secret

# Notion Database ID
DATABASE_ID=your-database-id

# Webhook API Key (任意の安全な文字列を生成してください)
WEBHOOK_API_KEY=your-webhook-api-key

# GCP Function Console URL (デプロイ後に設定)
GCP_FUNCTION_CONSOLE_URL=https://console.cloud.google.com/functions/details/region/your-function-name?project=your-project-id 
